<div class="tab_box">
    <div class="tabs_row">
        <% var activeContent = null %>  
        <% if (tabs) { %>  
            <% for (key in tabs) { %>
                <span id="<%= key %>" 
                    title="<%= tabs[key].hover %>" 
                    class="zui-tab <%= tabs[key].disabled === true ? 'disabled' : '' %>">
                        <%= tabs[key].label %>
                </span>
            <% };%>
        <% }%>
    </div>
    <div class="tabs_content_row"></div>
</div>